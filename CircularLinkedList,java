package practice;

public class CircularLinkedList {
    
    public class Node{
        private int value;
        private Node next;

        public Node(int v, Node n){
            value=v;
            next=n;
        }

        public Node(int v){
            value=v;
            next=null;
        }
    }

    private Node tail;
    private int size=0;

    public void addHead(int value){
        Node temp=new Node(value, null);
        if (size==0) {
            tail=temp;
            temp.next=temp;
        }
        else{
            temp.next=tail.next;
            tail.next=temp;
        }
        size++;
    }

    public void print() {
        Node temp = tail.next;
        while (temp != tail) {
            System.out.print(temp.value + " ");
            temp = temp.next;
        }
        System.out.print(temp.value);
    }

    public static void main(String[] args) {
        CircularLinkedList cll1=new CircularLinkedList();
        cll1.addHead(500);
        cll1.print();
    }
}
